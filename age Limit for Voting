#include<stdio.h>
int main(){
	int age;
	printf("Enter your age:\n");
	scanf("%d",&age);
	printf("You have entered %d as your age \n", age);
	
	if(age>=18){
		printf("You are eligible for voting!");
	}
	else if(age>10){
		printf("You age is between 10 to 17. so you cannot eligible for voting!");
	}else{
		printf("You are not eligible for voting!");
	}
	return 0;
}
